[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
	lgg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lg = log --graph --oneline
	last = 'log -3'
	rb = rebase
    rba = rebase --abort
    rbc = rebase --continue
    cp = cherry-pick 
    cpa = cherry-pick --abort 
    cpc = cherry-pick --continue 
	gp = pull --rebase github develop
	gps = push origin develop
	grc = rebase --continue
	lggg = log --graph --oneline -10
[user]
	name = Alex Xie
	email = xieweialex@gmail.com
[alia]
	gps = push origin develop
	grc = rebase --continue
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[core]
	excludesfile = /Users/Alex_Xie/.gitignore
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = current
[pull]
	default = current
